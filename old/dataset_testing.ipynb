     "text": [
      "class Solution:\n",
      "    def lengthOfLongestSubstring(self, s: str) -> int:\n",
      "        ss = set()\n",
      "        i = ans = 0\n",
      "        for j, c in enumerate(s):\n",
      "            while c in ss:\n",
      "                ss.remove(s[i])\n",
      "                i += 1\n",
      "            ss.add(c)\n",
      "            ans = max(ans, j - i + 1)\n",
      "        return ans\n",
      "\n"
     ]
    }
